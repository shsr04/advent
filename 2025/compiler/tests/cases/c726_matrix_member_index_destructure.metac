@Test({
   "exit" : 0,
   "stdout" : "Result: 32\n"
})
function main() {
  const result = run()?
  log("Result: ${result}")
}

function run(): number | error {
  let grid: matrix(number) with dim(2), matrixSize([2, 2]) = []
  grid.insert(10, [0, 1])
  grid.insert(20, [1, 0])

  let sum = 0
  for const cell in grid.members() {
    const [row, col] = cell.index()
    sum += row + col + cell
  }
  return sum
}
