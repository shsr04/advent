function main() {
  printf("Result: %d\n", solve() or (e) => {
    printf("Error! %s\n", e.message)
    return 1
  })
}

function hasRepeatingDigits(input: string): boolean {
  if input.chunk(input.size() / 2).size() != 2 {
    return 1 == 0
  }
  const [head, tail] = input.chunk(input.size() / 2)
  return head == tail
}

function solve(): number | error {
  let score = 0

  if hasRepeatingDigits("123123") {
    score += 1
  }
  if hasRepeatingDigits("1212") {
    score += 1
  }
  if hasRepeatingDigits("12345") {
    score += 10
  }
  if hasRepeatingDigits("111") {
    score += 100
  }

  return score
}
