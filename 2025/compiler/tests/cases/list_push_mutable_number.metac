@Test({
   "exit" : 0,
   "stdout" : "Result: 6\n"
})
function main() {
  const result = run()?
  log("Result: ${result}")
}

function run(): number | error {
  let values: number[] = []
  values.push(4)
  values.push(2)

  let sum = 0
  for const value in values {
    sum += value
  }

  return sum
}
