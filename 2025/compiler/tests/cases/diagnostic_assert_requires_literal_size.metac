@Test({
   "compile_err" : "assert(...) first arg must be a single-parameter lambda predicate"
})
function main() {
  const result = solve()?
  log("Result: ${result}")
}

function solve(): number | error {
  const expected = 2
  const bounds = split("10-12", "-")?
    .map(parseNumber)?
    .assert(expected, "Invalid range expression")?
  const [start, end] = bounds
  return start + end
}
