function main() {
  const result = probe()?
  log("Result: ${result}")
}

function probe(): number | error {
  let score = 0
  if hasRepeatingDigits("123123") {
    score += 10
  }
  if hasRepeatingDigits("123412") {
    score += 1
  }
  if hasRepeatingDigits("111111") {
    score += 100
  }
  return score
}

function hasRepeatingDigits(input: string): boolean {
  const size = input.size()
  for const divisor in seq(1, size-1).filter(x => size % x == 0) {
    let valid = true
    for const i in seq(divisor, size-1) {
      if input[i] != input[i-divisor] {
        valid = false
        break
      }
    }
    if valid {
      return true
    }
  }
  return false
}
