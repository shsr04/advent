function main() {
  printf("Result: %d\n", run() or (e) => {
    printf("Error! %s\n", e.message)
    return 1
  })
}

function run(): number | error {
  let best: number | null = null

  for const n in seq(1, 5) {
    if best == null || n > best {
      best = n
    }
  }

  if best == null {
    return error("missing best")
  } else {
    return best
  }
}
