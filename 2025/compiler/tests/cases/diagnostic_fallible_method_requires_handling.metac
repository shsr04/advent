@Test({
   "compile_err" : "Method 'map(...)' is fallible for mapper 'parseNumber'; handle it with '?'"
})
function main() {
  const result = solve()?
  log("Result: ${result}")
}

function solve(): number | error {
  const pieces = split("1,2,3", ",")?
  const nums = pieces.map(parseNumber)
  return nums.size()
}
