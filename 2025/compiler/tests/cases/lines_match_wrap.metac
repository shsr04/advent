@Test({
   "exit" : 0,
   "stdin" : "L2\nR5\nL1\n",
   "stdout" : "Result: 2\n"
})
function main() {
  const result = solve()?
  log("Result: ${result}")
}

function solve(): number | error {
  let dial: number with range(0,9) + wrap = 0

  for const line in lines(STDIN)? {
    const [direction, amount] = match(line, /(L|R)([0-9]+)/)?
    if direction == "L" {
      dial = dial - amount
    } else {
      dial = dial + amount
    }
  }

  return dial
}
