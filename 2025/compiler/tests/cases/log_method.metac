@Test({
   "exit" : 0,
   "stdout" : "start\n[1, 2]\n2\n1\n2\nResult: 3\n"
})
function main() {
  const result = solve()?
  log("Result: ${result}")
}

function solve(): number | error {
  log("start")
  const nums = "12".chars().map(parseNumber)?
  nums.log()

  const peak = nums.max().log()
  const idx = peak.index().log()
  const echoed = log(peak)

  return echoed + idx
}
