function main() {
  const result = solve()?
  log("Result: ${result}")
}

function solve(): number | error {
  log("start")
  const nums = "12".chars().map(parseNumber)?
  nums.log()

  const peak = nums.max().log()
  const idx = peak.index().log()
  const echoed = log(peak)

  return echoed + idx
}
