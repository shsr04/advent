@Test({
   "compile_err" : "Method 'insert(...)' is fallible on unconstrained matrix; handle it with '?'"
})
function main() {
  const result = run()?
  log("Result: ${result}")
}

function run(): number | error {
  let grid: matrix(number) with dim(2) = []
  const next = grid.insert(1, [0, 0])
  return next.members().size()
}
