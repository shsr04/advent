function main() {
  const result = solve()?
  log("Result: ${result}")
}

function toLen(s: string): number {
  return s.size()
}

function solve(): number | error {
  const parts = split("aa,b,ccc", ",")?
  const lens = parts.map(toLen)

  let total = 0
  for const n in lens {
    total += n
  }

  return total
}
