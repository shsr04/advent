function main() {
  printf("Result: %d\n", run() or (e) => {
    printf("Error! %s\n", e.message)
    return 1
  })
}

function run(): number | error {
  let grid: matrix(number) with dim(2) = []
  const grid1 = grid.insert(1, [0, 0])?
  const grid2 = grid1.insert(2, [0, 1])?
  return grid2.members().size()
}
