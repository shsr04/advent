@Test({
   "exit" : 0,
   "stdout" : "Result: 33\n"
})
function main() {
  const result = solve()?
  log("Result: ${result}")
}

function solve(): number | error {
  const bounds = split("10-12", "-")?
    .map(parseNumber)?
    .assert(x => x.size() == 2, "Invalid range expression")?

  const [start, end] = bounds
  let total = 0

  for const x in seq(start, end) {
    total += x
  }

  return total
}
