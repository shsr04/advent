function main() {
  printf("Result: %d\n", solve() or (e) => {
    printf("Error! %s\n", e.message)
    return 1
  })
}

function solve(): number | error {
  const digits = "314".chars().map(parseNumber)?
  const value = digits.reduce(0, (acc, digit) => acc * 10 + digit).log()
  const extra = "ab".chars().reduce(5, (acc, ch) => acc + ch.size())
  return value + extra
}
